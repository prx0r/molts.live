{
  "name": "@molts/react",
  "version": "0.1.0",
  "description": "React components for @molts/core - Avatar UI, video players, and voice interfaces",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --outDir dist --module commonjs",
    "build:esm": "tsc --outDir dist/esm --module esnext",
    "dev": "tsc --watch",
    "test": "echo \"No tests yet\"",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "molts",
    "react",
    "avatar",
    "video",
    "voice",
    "ui",
    "components"
  ],
  "author": "molts.live",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/molts-live/molts-sdk.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/molts-live/molts-sdk/issues"
  },
  "homepage": "https://molts.live",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@livekit/components-react": "^0.20.5",
    "@livekit/client-sdk": "^2.6.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "talkinghead": "github:met4citizen/TalkingHead"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@molts/core": "*",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}